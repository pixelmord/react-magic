type File {
  contentType: String!
  createdAt: DateTime!
  id: ID!
  name: String!
  secret: String!
  size: Int!
  updatedAt: DateTime!
  url: String!
}

type Section {
  createdAt: DateTime!
  description: String
  id: ID!
  imageUrl: String
  name: String!
  updatedAt: DateTime!
}

type User {
  createdAt: DateTime!
  id: ID!
  updatedAt: DateTime!
}